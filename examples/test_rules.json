{
    "rules": [
        {
            "expectation_type": "expect_column_values_to_be_between",
            "kwargs": {
                "column": "quantity",
                "min_value": 0,
                "max_value": 1000
            }
        },
        {
            "expectation_type": "expect_column_values_to_match_regex",
            "kwargs": {
                "column": "date",
                "regex": "^\\d{4}-\\d{2}-\\d{2}$"
            }
        },
        {
            "expectation_type": "expect_column_values_to_be_between",
            "kwargs": {
                "column": "unit_price",
                "min_value": 0,
                "max_value": 100
            }
        }
    ]
}
